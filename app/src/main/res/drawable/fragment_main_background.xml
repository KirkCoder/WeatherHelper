<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="rectangle">
            <gradient
                    android:startColor="#4075FF"
                    android:endColor="#CB7B82FF"
                    android:type="linear"/>
        </shape>
    </item>
    <item>
        <shape
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="rectangle">
            <gradient
                    android:angle="90"
                    android:startColor="#7D637EFF"
                    android:endColor="#cbeb93ff"
                    android:type="linear"/>
        </shape>
    </item>
</layer-list>